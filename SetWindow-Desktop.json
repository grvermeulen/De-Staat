{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "print",
      "text": "SCHERMGROOTTE DESKTOP + ACHTERGRONDCHECK"
    },
    {
      "type": "setWindowSize",
      "width": "1024",
      "height": "1080"
    },
    {
      "type": "refresh"
    },
    {
      "type": "storeCurrentUrl",
      "variable": "url"
    },
    {
      "type": "print",
      "text": "${url}"
    },
    {
      "type": "verifyElementStyle",
      "locator": {
        "type": "id",
        "value": "main"
      },
      "propertyName": "background-image",
      "value": "url(\"${url}sites/default/files/achtergrond.jpg\")"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}