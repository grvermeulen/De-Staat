{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://webcmsl02-ext-t.rivm.nl/staat/"
    },
    {
      "type": "print",
      "text": "VUL ZOEKTERM KERNCIJFER ZOEKBOX"
    },
    {
      "type": "storeText",
      "locator": {
        "type": "css selector",
        "value": "#block-views-0119fa2142bb85e245be366fd7ae05f7 > h2"
      },
      "variable": "autocomplete"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "//input[@id='edit-search-api-views-fulltext']"
      },
      "text": "${autocomplete}"
    },
    {
      "type": "waitForTextPresent",
      "text": "${autocomplete}"
    },
    {
      "type": "print",
      "text": "Mijn zoekterm is ${autocomplete}"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "edit-submit-ds-kernvragen-search"
      }
    },
    {
      "type": "print",
      "text": "CONTROLEER TITEL"
    },
    {
      "type": "verifyTextPresent",
      "text": "Overzicht Kerncijfers"
    },
    {
      "type": "verifyElementPresent",
      "locator": {
        "type": "xpath",
        "value": "//div/div/div[2]/div[1]/h1"
      }
    },
    {
      "type": "print",
      "text": "ZOEKBOX ELEMENT, ZOEKBOXTEKST"
    },
    {
      "type": "verifyElementPresent",
      "locator": {
        "type": "xpath",
        "value": "//div/div/div[2]/div[2]/div[1]/section/form/div/div/div/div[1]/div/div/input[1]"
      }
    },
    {
      "type": "verifyElementValue",
      "locator": {
        "type": "id",
        "value": "edit-search-api-views-fulltext"
      },
      "value": "${autocomplete}"
    },
    {
      "type": "print",
      "text": "RESULTATEN OVERZICHT"
    },
    {
      "type": "print",
      "text": "CONTROLEER #RESULTATEN, RESULTATEN LIJST"
    },
    {
      "type": "verifyTextPresent",
      "text": "resultaten"
    },
    {
      "type": "storeElementValue",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='content']//b[.='Kerncijfers']"
      },
      "variable": "resultaat"
    },
    {
      "type": "verifyElementPresent",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='content']/div[3]/div[2]/ol/li[1]"
      }
    },
    {
      "type": "verifyText",
      "locator": {
        "type": "id",
        "value": "edit-search-api-views-fulltext"
      },
      "text": "${autocomplete)"
    },
    {
      "type": "print",
      "text": "SELECTEER 1e RESULTAAT"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "${autocomplete}"
      }
    },
    {
      "type": "print",
      "text": "KERNCIJFER DETAIL PAGINA MET H1 EN TEKST"
    },
    {
      "type": "verifyElementPresent",
      "locator": {
        "type": "xpath",
        "value": "//div/div/div[2]/div[1]/h1"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "${autocomplete}"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}