{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "img.logotype"
      }
    },
    {
      "type": "print",
      "text": "VUL ZOEKTERM KERNCIJFER ZOEKBOX"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "//input[@id='edit-search-api-views-fulltext']"
      },
      "text": "Kerncijfers voor beleid"
    },
    {
      "type": "storeElementValue",
      "locator": {
        "type": "xpath",
        "value": "//input[@id='edit-search-api-views-fulltext']"
      },
      "variable": "autocomplete"
    },
    {
      "type": "print",
      "text": "Mijn zoekterm is ${autocomplete}"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "edit-submit-ds-kernvragen-search"
      }
    },
    {
      "type": "print",
      "text": "CONTROLEER TITEL"
    },
    {
      "type": "verifyTextPresent",
      "text": "Overzicht Kerncijfers"
    },
    {
      "type": "verifyElementPresent",
      "locator": {
        "type": "xpath",
        "value": "//div/div/div[2]/div[1]/h1"
      }
    },
    {
      "type": "print",
      "text": "ZOEKBOX ELEMENT, ZOEKBOXTEKST"
    },
    {
      "type": "verifyElementPresent",
      "locator": {
        "type": "id",
        "value": "edit-search-api-views-fulltext"
      }
    },
    {
      "type": "verifyElementValue",
      "locator": {
        "type": "id",
        "value": "edit-search-api-views-fulltext"
      },
      "value": "${autocomplete}"
    },
    {
      "type": "print",
      "text": "RESULTATEN OVERZICHT"
    },
    {
      "type": "print",
      "text": "CONTROLEER #RESULTATEN, RESULTATEN LIJST"
    },
    {
      "type": "verifyTextPresent",
      "text": "resultaten"
    },
    {
      "type": "storeElementValue",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='content']/div[2]/div[2]/ol/li[1]"
      },
      "variable": "resultaat"
    },
    {
      "type": "verifyElementPresent",
      "locator": {
        "type": "xpath",
        "value": "//div/div/div[2]/div[2]/div[2]/div[2]/ol/li[1]"
      }
    },
    {
      "type": "verifyText",
      "locator": {
        "type": "id",
        "value": "edit-search-api-views-fulltext"
      },
      "text": "${autocomplete)"
    },
    {
      "type": "print",
      "text": "SELECTEER 1e RESULTAAT"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "${autocomplete}"
      }
    },
    {
      "type": "print",
      "text": "KERNCIJFER DETAIL PAGINA MET H1 EN TEKST"
    },
    {
      "type": "verifyText",
      "locator": {
        "type": "xpath",
        "value": "//div/div/div[2]/div[1]/h1"
      },
      "text": "${autocomplete}"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "img.logotype"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//input[@id='edit-search-api-views-fulltext']"
      }
    },
    {
      "type": "storeElementValue",
      "locator": {
        "type": "id",
        "value": "edit-search-api-views-fulltext"
      },
      "variable": "placeholdertekst"
    },
    {
      "type": "print",
      "text": "${placeholdertekst}"
    },
    {
      "type": "print",
      "text": "PLACEHOLDER ZOEKBOX HEEFT INHOUD:"
    },
    {
      "type": "verifyElementValue",
      "negated": true,
      "locator": {
        "type": "xpath",
        "value": "//input[@id='edit-search-api-views-fulltext']"
      },
      "value": ""
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}