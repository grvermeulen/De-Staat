{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://webcmsl02-ext-t.rivm.nl/staat/"
    },
    {
      "type": "get",
      "url": "http://webcmsl02-ext-t.rivm.nl/staat/user"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "edit-name"
      },
      "text": "admin"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "edit-pass"
      },
      "text": "admin"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "edit-submit"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "#admin-menu-menu > li.admin-menu-toolbar-category.expandable > a"
      }
    },
    {
      "type": "switchToFrameByIndex",
      "index": "1"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='views-widget']/div/input"
      },
      "text": "zorg"
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='views-widget']/div/select//option[6]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//input[@class='form-submit']"
      }
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}